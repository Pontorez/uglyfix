{
    "content_scripts": [
        {
            "matches": ["http://www.netvibes.com/privatepage/*"],
            "css": ["css/netvibes.com.css"],
            "js": [
                "js/jquery-2.1.0.min.js",
                "js/netvibes.com.js"
            ]
        },
        {
            "matches": ["https://www.google.ru/*", "https://www.google.de/*"],
            "css": ["css/google.css"],
            "js": ["js/google_search.js"]
        },
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["js/mail.google.com.js"]
        },
        {
            "matches": ["http://www.kcblog.info/*"],
            "js": [
                "js/kcblog.info.js"
            ]
        },
        {
            "matches": ["http://*.newsru.com/*"],
            "js": [
                "js/jquery-2.1.0.min.js",
                "js/newsru.com.js"
            ],
            "css": [
                "css/newsru.com.css"
            ]
        },
        {
            "matches": ["http://*.livejournal.com/*"],
            "css": [
                "css/livejournal.com.css"
            ]
        },
        {
            "matches": ["http://blogarbik.ru/*"],
            "css": [
                "css/blogarbik.ru.css"
            ]
        },
        {
            "matches": ["https://*.wikipedia.org/w*"],
            "js": [
                "js/jquery-2.1.0.min.js",
                "js/wikipedia.js"
            ]
        },
        {
            "matches": ["http://antizapret.info/*"],
            "css": ["css/antizapret.info.css"],
            "js": [
                "js/jquery-2.1.0.min.js",
                "js/antizapret.info.js"
            ]
        },
        {
            "matches": ["https://inoreader.com/"],
            "css": [
                "css/inoreader.css"
            ]
        },
        {
            "matches": ["http://dooma.ru/images/*"],
            "js": [
                "js/dooma.ru.js"
            ]
        },
        {
            "matches": ["http://hh.ru/*"],
            "css": ["css/hh.ru.css"]
        },
        {
            "matches": ["http://career.ru/*"],
            "css": ["css/career.ru.css"]
        },
        {
            "matches": ["http://inoreader.com/*", "http://*.wikipedia.org/*", "http://lj.rossia.org/*", "http://stat.netbynet.ru/*", "http://translate.google.com/*"],
            "js": ["js/https.js"],
            "run_at": "document_start"
        }
    ],
    "name": "UglyFix",
    "description": "Fixes for some ugly sites (Mar 10, 2014)",
    "version": "1.9",
    "manifest_version": 2
}
