{
    "content_scripts": [
        {
            "matches": ["https://www.google.com/*", "https://www.google.ru/*", "https://www.google.de/*"],
            "css": ["css/google.css"],
            "js": ["js/google_search.js"]
        },
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["js/mail.google.com.js"]
        },
        {
            "matches": ["https://www.youtube.com/*"],
            "css": ["css/youtube.css"]
        },
        {
            "matches": ["http://www.kcblog.info/*"],
            "js": [
                "js/kcblog.info.js"
            ]
        },
        {
            "matches": ["*://*.newsru.com/*"],
            "css": [
                "css/newsru.com.css"
            ],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/newsru.com.js"
            ]
        },
        {
            "matches": ["http://*.livejournal.com/*"],
            "css": [
                "css/livejournal.com.css"
            ]
        },
        {
            "matches": ["https://*.wikipedia.org/w*"],
            "css": ["css/wikipedia.css"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/wikipedia.js"
            ]
        },
        {
            "matches": ["https://antizapret.info/*"],
            "css": ["css/antizapret.info.css"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/antizapret.info.js"
            ]
        },
        {
            "matches": ["https://inoreader.com/*", "https://www.inoreader.com/*"],
            "css": [
                "css/inoreader.css"
            ],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/inoreader.js"
            ]
        },
        {
            "matches": ["http://dooma.ru/images/*"],
            "js": [
                "js/dooma.ru.js"
            ]
        },
        {
            "matches": ["https://*.hh.ru/*", "http://*.hh.ua/*", "http://*.hh.kz/*", "http://jobs.tut.by/*", "http://jobs.day.az/*"],
            "css": ["css/hh.ru.css"]
        },
        {
            "matches": ["https://career.ru/*"],
            "css": ["css/career.ru.css"]
        },
        {
            "matches": ["*://geektimes.ru/*", "*://habrahabr.ru/*"],
            "css": ["css/habrahabr_geektimes.css"]
        },
        {
            "matches": [
                "http://archive.org/*",
                "http://inoreader.com/*", "http://www.inoreader.com/*",
                "http://www.gibdd.ru/*",
                "http://www.intimcity.nl/*",
                "http://www.kavkazcenter.com/*",
                "http://lj.rossia.org/*",
                "http://lurkmore.to/*",
                "http://www.newsru.com/*",
                "http://rutracker.org/*",
                "http://sextalk.ru/*",
                "http://stat.netbynet.ru/*",
                "http://stackoverflow.com/*",
                "http://thepiratebay.se/*",
                "http://translate.google.com/*",
                "http://vk.com/*",
                "http://www.urbandictionary.com/*",
                "http://www.youtube.com/*",
                "http://*.wikipedia.org/*"
            ],
            "js": ["js/https.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["http://www.gibdd.ru/check/fines/*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/gibdd.ru.js"
            ]
        },
        {
            "matches": ["https://www.intimcity.nl/*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/intimcity.nl.js"
            ]
        },
        {
            "matches": ["https://lj.rossia.org/*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/lj.rossia.org.js"
            ]
        },
        {
            "matches": ["https://*.pl/owa/*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/outlook.js"
            ]
        },
        {
            "matches": ["https://login.skype.com/login?*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/skype.com.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": ["https://www.4proxy.us/*"],
            "js": ["js/4proxy.us.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["https://mail.protonmail.com/*"],
            "js": [
                "js/jquery-3.1.1.min.js",
                "js/protonmail.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": ["https://www.youtube.com/watch?v=*"],
            "js": [
                "js/youtube.js"
            ],
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": [
            "js/jquery-3.1.1.min.js",
            "js/pure_url.js",
            "js/background.js"
        ]
    },
    "permissions": ["webRequest", "webRequestBlocking", "\u003Call_urls>"],
    "externally_connectable": {
        "matches": [
            "https://mail.protonmail.com/*"
        ]
    },
    "name": "UglyFix",
    "manifest_version": 2,
    "homepage_url": "https://github.com/Pontorez/uglyfix",
    "icons": {
        "48": "img/icon48.png"
    },
    "description": "Fixes for some ugly sites (Mar 4, 2017)",
    "version": "2.70"
}
